{
    "name": "appium-starter",
    "version": "1.0.0",
    "description": "A simple Cordova app showing how to test using Appium, WebdriverIO and Jasmine framework",
    "private": true,
    "scripts": {
        "postinstall": "node ./scripts/post-install.js",
        "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
        "clean": "rimraf dist/ && rimraf logs/**/*.log  && rimraf tmp/**/*.*",
        "clean-build": "npm run clean && npm run build",
        "pretest": "npm run clean-build",
        "test": "node config/app.js",
        "posttest": "npm run allure-report-dev",
        "dev": "npm run clean-dev-report && npm run test",
        "master": "npm run pretest && node config/strategies.master/app.master.js",
        "release": "npm run pretest && node config/strategies.release/app.migration.js",
        "master-new-user": "",
        "release-new-user": "",
        "smoke": "npm run clean-smoke-report && npm run pretest && node config/strategies.smoke/app.smoke.js",
        "fullmigrationtest-release": "npm run pretest && npm run full-release",
        "smokemigrationtest": "npm run pretest && npm run smoke",
        "prettier": "prettier --config .prettierrc --write \"{src,config}/**/*.{ts,js,json}\"",
        "allure-report-release": "allure generate ./reports/release/allure-results-release --clean -o ./reports/release/allure-report",
        "allure-history-release": "mkdir -p ./reports/release/allure-report/history ./reports/release/allure-results-release/history && cp -a ./reports/release/allure-report/history ./reports/release/allure-results-release/",
        "open-report-release": "allure open ./reports/release/allure-report",
        "releaseMigrationReport": "npm run allure-report-release && npm run allure-open-report-release",
        "allure-report-master": "allure generate ./reports/master/allure-results-master -o ./reports/master/allure-report --clean",
        "allure-history-master": "mkdir -p ./reports/master/allure-report/history ./reports/master/allure-results-master/history && cp -a ./reports/master/allure-report/history ./reports/master/allure-results-master/",
        "open-report-master": "allure open ./reports/master/allure-report",
        "masterMigrationReport": "npm run allure-report-master && npm run allure-open-report-master",
        "allure-report-dev": "allure generate ./reports/dev/allure-results-dev --clean -o ./reports/dev/allure-report",
        "open-report-dev": "allure open ./reports/dev/allure-report",
        "clean-release-report": "rimraf reports/release/allure-results-release",
        "clean-master-report": "rimraf reports/master/allure-results-master",
        "clean-dev-report": "rimraf reports/dev/allure-results-dev",
        "clean-smoke-report": "rimraf config/strategies.smoke/reports",
        "appium-doctor": "appium-doctor --ios",
        "clean-screenshots": "rimraf screenshots.dev/**/*.*"
    },
    "author": "Hanuman Team",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/mobiliarmad/e2e-test-with-appium.git"
    },
    "dependencies": {
        "@slack/webhook": "^5.0.2",
        "appium": "^1.20.2",
        "appium-support": "^2.35.1",
        "dotenv": "^8.0.0",
        "ios-webkit-debug-proxy": "0.0.3",
        "tscpaths": "0.0.9",
        "webdriverio": "^5.16.2"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/jasmine": "^3.3.12",
        "@types/lodash": "^4.14.168",
        "@types/node": "^12.0.2",
        "@wdio/allure-reporter": "^5.22.4",
        "@wdio/appium-service": "^5.13.2",
        "@wdio/cli": "5.16.2",
        "@wdio/config": "5.16.2",
        "@wdio/dot-reporter": "5.15.2",
        "@wdio/jasmine-framework": "5.16.0",
        "@wdio/local-runner": "5.16.2",
        "@wdio/spec-reporter": "5.15.2",
        "@wdio/sync": "5.16.0",
        "allure-commandline": "^2.13.0",
        "appium-doctor": "1.10.0",
        "babel-plugin-istanbul": "^5.1.1",
        "chalk": "2.4.2",
        "date-fns": "^2.16.1",
        "istanbul-api": "^2.1.1",
        "istanbul-lib-coverage": "^2.0.3",
        "istanbul-lib-report": "^2.0.4",
        "istanbul-reports": "^2.1.1",
        "lodash": "^4.17.21",
        "prettier": "^2.1.2",
        "rimraf": "^2.6.3",
        "shelljs": "0.8.3",
        "ts-lint": "^4.5.1",
        "ts-node": "^8.1.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.2",
        "wdio-json-reporter": "^2.0.0",
        "wdio-slack-service": "^2.0.8"
    },
    "optionalDependencies": {
        "fsevents": "^2.0.7",
        "fs-extra": "^8.0.1"
    },
    "engines": {
        "node": ">= 8.x.x",
        "npm": ">= 5.x.x"
    },
    "keywords": [
        "appium",
        "webdriverio",
        "jasmine framework",
        "typescript",
        "mobile automation"
    ],
    "bundledDependencies": [
        "@slack/webhook",
        "appium",
        "appium-support",
        "dotenv",
        "lodash",
        "tscpaths",
        "webdriverio"
    ]
}
